{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "permissions": {
    "allow": [
      "Bash(/bin/ls:*)",
      "Bash(/usr/bin/ls:*)",
      "Bash(cargo build:*)",
      "Bash(cargo check:*)",
      "Bash(cargo doc:*)",
      "Bash(cargo info:*)",
      "Bash(cargo run:*)",
      "Bash(cargo test:*)",
      "Bash(git mv:*)",
      "Bash(go build:*)",
      "Bash(go test:*)",
      "Bash(ls:*)",
      "Bash(trunk check:*)",
      "Bash(trunk fmt:*)",
      "Read(~/.config/agents/*.md)",
      "Read(~/.config/agents/*/*.md)",
      "WebFetch(domain:crates.io)",
      "WebFetch(domain:docs.renovatebot.com)",
      "WebFetch(domain:docs.rs)",
      "WebFetch(domain:github.com)",
      "WebSearch"
    ]
  },
  "fileSuggestion": {
    "type": "command",
    "command": "{{ .chezmoi.homeDir }}/.claude/scripts/file-suggestion.sh"
  },
  "statusLine": {
    "type": "command",
    "command": "{{ .chezmoi.homeDir }}/.claude/scripts/statusline.sh"
  },
  "alwaysThinkingEnabled": true,
  "preferences": {
    "theme": "dark",
    "autoSave": true
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754077213482
  },
  "enabledPlugins": {
    "superpowers@superpowers-dev": true
  }
}
