{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>scottames/renovate:default.json5"
  ],
  "ignorePaths": [
    ".dagger/go.mod"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "home/private_dot_config/k9s/private_config.yaml.tmpl"
      ],
      "matchStrings": [
        "renovate:(\sdatasource=(?<datasource>.*?))?\sdepName=(?<depName>.*?)s*\n\s+image:\s(?<depName>.*?):(?<currentValue>.*)"
      ],
      "replaceString": "{{- /* renovate: datasource={{{datasource}}} depName={{{depName}}} */}}\n    image: {{{depName}}}:{{#if newValue}}{{{newValue}}}{{else}}{{{currentValue}}}{{/if}}{{#if newDigest}}@{{{newDigest}}}{{/if}}"
    }
  ]
}